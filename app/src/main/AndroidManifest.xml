<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bgeiotdev.eval">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewHoldUser" >
            <intent-filter>
                <action android:name="com.bgeiotdev.action.NewHoldUser" />

                <category android:name="com.bgeiotdev.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".GameDifficultyActivity" >
            <intent-filter>
                <action android:name="com.bgeiotdev.action.GameDifficultyActivity" />

                <category android:name="com.bgeiotdev.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".GameActivity" >
            <intent-filter>
                <action android:name="com.bgeiotdev.action.GameActivity" />

                <category android:name="com.bgeiotdev.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".InstructionsActivity" >
            <intent-filter>
                <action android:name="com.bgeiotdev.action.InstructionsActivity" />

                <category android:name="com.bgeiotdev.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ChooseNumberActivity" >
            <intent-filter>
                <action android:name="com.bgeiotdev.action.ChooseNumberActivity" />

                <category android:name="com.bgeiotdev.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".PageScoring" >
            <intent-filter>
                <action android:name="com.bgeiotdev.action.PageScoring" />

                <category android:name="com.bgeiotdev.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>
</manifest>